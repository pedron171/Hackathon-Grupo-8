{
  "name": "api-node",
  "version": "1.0.0",
  "description": "API do Hackathon - Grupo 8",
  "main": "src/server.ts",
  "type": "commonjs",
  "scripts": {
    "migrate": "ts-node ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "migrate:make": "ts-node ./node_modules/knex/bin/cli.js migrate:make --knexfile knexfile.ts",
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn src/server.ts"
  },
  "keywords": [],
  "author": "Grupo 8 - UniALFA Hackathon",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.4",
    "knex": "^2.5.1",
    "mysql2": "^3.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/node": "^20.11.30",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
